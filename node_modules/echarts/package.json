    "seedrandom": "3.0.3",
    "semver": "6.3.0",
    "serve-handler": "6.1.1",
    "slugify": "1.3.4",
    "socket.io": "2.2.0",
    "terser": "^5.3.8",
    "ts-jest": "^26.4.3",
    "typescript": "4.4.3"
  },
